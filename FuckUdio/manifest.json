{
  "manifest_version": 3,
  "name": "Fuck Udio Downloader",
  "version": "1.0.1",
  "description": "Downloads the currently playing <audio> element's MP3 with one click.",
  "action": {
    "default_title": "Fuck Udio"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}